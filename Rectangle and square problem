#include <stdio.h>

int main() {
	// your code goes here
     int a,b,c;
     scanf("%d %d %d",&a,&b,&c);
     int rectangle,square;
     rectangle = a*b;
     square = c*c;
     if(rectangle==square)
     {
         printf("Yes");
     }
     else
     {
         printf("No");
     }
     return 0;
}
